  "bugs": {
    "url": "https://github.com/sumit194/graphql-tutorial/issues"
  },
  "homepage": "https://github.com/sumit194/graphql-tutorial#readme",
  "dependencies": {
    "@2fd/graphdoc": "^2.4.0",
    "bcrypt": "^2.0.1",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "graphql": "^0.13.2",
    "jsonwebtoken": "^8.2.1",
    "lodash": "^4.17.10"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",

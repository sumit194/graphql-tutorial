{
  "name": "graphql-tutorial",
  "version": "1.0.0",
  "description": "GraphQL Express Node Server created for learning purpose",
  "main": "server.js",
  "graphdoc": {
    "endpoint": "http://localhost:4040/api",
    "output": "./doc/schema"
  },
  "scripts": {
    "babel-node": "babel-node --presets=env",
    "start": "nodemon --exec npm run babel-node -- server.js",
